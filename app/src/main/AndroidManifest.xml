<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techweezy.mobifarm">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.LogOut"></activity>
        <activity android:name=".activities.AssignLot" />
        <activity android:name=".activities.HomeScreen" />
        <activity android:name=".activities.FundFarmer" />
        <activity android:name=".activities.Aprv" />
        <activity android:name=".activities.ViewRequests" />
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".activities.RegisterScreen" />
        <activity android:name=".activities.CheckDetails" />
        <activity
            android:name=".activities.LoginScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>